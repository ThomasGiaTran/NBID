@model NBID.Models.HomeViewModels

<table id="dtable" class="table table-striped table-bordered">
    <thead>
        <tr>
            @*<th style="text-align:center">
                <button id="btnMerge" class="btn btn-sm btn-primary">
                    MERGE
                </button>
            </th>*@
            <th>NBID</th>
            <th>Account</th>
            <th>Name</th>
            <th>Address</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.queryList)
            {
            <tr>
                @*<td style="text-align:center;"><input type="checkbox" name="QResults" value="@item.nbid"></td>*@
                <td><a href="#" id="Results">@item.nbid</a></td>
                <td>@item.acctno</td>
                <td>@item.cust_name</td>
                <td>@item.address1 &nbsp; @item.address2</td>
                <td>@item.phone</td>
            </tr>
        }
    </tbody>
</table>
